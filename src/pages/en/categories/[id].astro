---
import CategoryPosts from "../../../layouts/CategoryPosts.astro";

export function getStaticPaths() {
  const categories = [
    {
      id: "computacao",
      name: "Computing",
    },
    {
      id: "desk",
      name: "Desk",
    },
  ];

  return categories.map(({ id, name }) => ({
    params: { id },
    props: { category: { id, name } },
  }));
}

const { category } = Astro.props;
---

<CategoryPosts category={category} />
